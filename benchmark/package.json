{
	"name": "benchmark",
	"packageManager": "pnpm@10.6.5",
	"scripts": {
		"lint": "turbo lint",
		"check-types": "turbo check-types",
		"format": "turbo format",
		"build": "turbo build",
		"build:extension": "cd .. && npm run vscode-test",
