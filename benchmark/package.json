	},
	"devDependencies": {
		"@dotenvx/dotenvx": "^1.39.0",
		"@eslint/js": "^9.22.0",
		"eslint": "^9.22.0",
		"globals": "^16.0.0",
		"prettier": "^3.5.3",
		"rimraf": "^6.0.1",
		"tsup": "^8.4.0",
		"tsx": "^4.19.3",
		"turbo": "^2.4.4",
		"typescript": "^5",
		"typescript-eslint": "^8.26.0"
	}
}
